{
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://127.0.0.1:8080/**" },
        { "url": "http://localhost:8080/**" }
      ]
    }
  ]
}

